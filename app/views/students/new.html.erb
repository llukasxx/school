<%= link_to "<< Back", admin_students_path, role: "botton", class: "btn btn-primary btn-lg"%> <br \> <br \> 
<%= simple_form_for @student, url: admin_students_path do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.button :submit %>
<% end %>